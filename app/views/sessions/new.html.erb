<%= link_to "Sign In", "#", class: "navbar-brand", id: "symbol" %>
<div class="container">
  <div class="row">
    <%= form_for(:session, :html => {class: "row centered-form", role: "form"}, url: login_path) do |f| %>
        <div class="row centered-form">
        <div class="col-xs-12 col-sm-8 col-md-4 col-sm-offset-2 col-md-offset-4">
        	<div class="panel panel-default">
        		<div class="panel-heading">
              <h4>User Sign In</h4>
			 			</div>
			 			<div class="panel-body">
			    		<form role="form">
			    			<div class="form-group">
                  <%= f.email_field :email, class: "form-control input-sm", placeholder: "Enter email", autofocus: true %>
			    			</div>
			    					<div class="form-group">
                      <%= f.password_field :password, class: "form-control input-sm", placeholder: "Enter password", autocomplete: "off" %>
			    					</div>
                <%= f.submit("Sign In", class: 'btn btn-info btn-block', id: "links", class: 'btn btn-primary btn-sm') %>
			    		</form>
			    	</div>
	    		</div>
    		</div>
    	</div>
  <% end %>
  </div>
  </div>
